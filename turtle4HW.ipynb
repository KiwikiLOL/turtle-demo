   "source": [
    "import turtle\n",
    "turtle.shape('turtle')\n",
    "my_turtle = turtle.Turtle()\n",
    "color=['pink','cyan','light green']\n",
    "for i in range (3):\n",
    "    my_turtle.color(color[i])\n",
    "    my_turtle.penup()\n",
    "    my_turtle.goto(-160, 100*i)\n",
    "    my_turtle.pendown()\n",
    "    my_turtle.forward(100+i*50)\n",
    "turtle.done()"
   ]
